class Solution(object):
    def isSubsequence(self, s, t):
        """
        :type s: str
        :type t: str
        :rtype: bool
        """
        while s:
            if s[0] in t:
                pos = t.index(s[0])
            else:
                return False
            t = t[pos+1:]
            s = s[1:]
        return True
            
            
        
